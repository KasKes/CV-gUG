---
import Base from "@/layouts/Base.astro";
import { getEntry } from "astro:content";
import TestimonialSlider from "@/layouts/react-partials/TestmonialSlider.jsx";
import type { PricingType } from "@/types";
import DynamicIcon from "@/helpers/DynamicIcon";
import PageHeader from "@/partials/PageHeader.astro";
import ImageMod from "@/components/ImageMod.astro";
const { data, render } = await getEntry("preise", "wedding-all");
const {
  title,
  meta_title,
  description,
  image,
  pricingTable,
  inklusiv,
  inklusivtitel,
  bilder,
} = data as PricingType;

const testimonials = (await getEntry("testimonial", "-index")).data
  .testimonial_items;
const weddingTestimonials = testimonials.filter(
  (item: any) => item.section === "wedding"
);
---

<Base
  title={title}
  meta_title={meta_title}
  description={description}
  keywords="Hochzeitsfotograf, Preise"
  image={image}
  body_class="page-color-dark-hochzeits page-color-light-hochzeits">
  <PageHeader title={title} />
  <section class="section overflow-hidden">
    <div class="container">
      <div class="row gy-5 gx-4 pb-6">
        <div class="md:col-4 relative">
          <h2 class="">Hochzeitsfotograf Preise</h2>
          Hochzeitsfotograf Preise • Qualität muss nicht überteuert sein.<br
          />Wir bieten Hochzeitsfotografie für jedes Budget an. Unsere drei
          Standardpakete können individuell ergänzt werden.
          <a
            data-te-nav-link-ref
            class="text-center mx-2 border-2 border-wedding rounded-xl hover:bg-wedding hover:text-bgweiss my-8 py-4 px-8 dark:border-wedding flex uppercase"
            href="#kennenlernshooting">
            Sichere dir jetzt dein kostenlosses Kennenlernshooting!
          </a>
          <!-- f/1 -->
          <svg
            class="pt-4 absolute top-0 right-0 -z-10"
            width="153"
            height="137"
            viewBox="0 0 153 137"
            fill="none"
            xmlns="http://www.w3.org/2000/svg">
            <path
              d="M44.1908 33.8009L42.7487 42.3985H0.925656L2.31236 33.8009H44.1908ZM1.203 119L17.4552 21.3206C18.1947 16.7352 19.8773 12.8709 22.5028 9.72772C25.1652 6.58452 28.3454 4.19938 32.0433 2.57232C35.7412 0.945252 39.5315 0.131719 43.4143 0.131719C46.0028 0.131719 48.1106 0.353592 49.7376 0.797339C51.3647 1.20411 52.6405 1.61087 53.5649 2.01764L49.3494 10.6707C48.6098 10.4488 47.7223 10.1899 46.6869 9.89412C45.6885 9.59829 44.3757 9.45038 42.7487 9.45038C38.4591 9.45038 34.9461 10.6522 32.2097 13.0558C29.4733 15.4594 27.7168 18.8984 26.9402 23.3729L11.0763 119H1.203ZM101.905 0.0762494L42.8319 136.084H33.5687L92.5868 0.0762494H101.905ZM152.797 5.4012L133.938 119H123.566L140.594 16.273H139.929L110.364 33.9673L112.195 23.2065L141.981 5.4012H152.797Z"
